		"2amigos/yii2-tinymce-widget": "*",
		"phpoffice/phpspreadsheet": "*",
		"sizeg/yii2-jwt": "^2.0",
		"wbraganca/yii2-dynamicform": "*",
		"borales/yii2-phone-input": "*",
